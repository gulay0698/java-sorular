Packagesoru;
	public class soru14 {
	public static void main(String[] args) {
	
	for(int i=1; i<=100; i++)
	{
	System.out.println(i + " rakamının/\nsayısının bölenleri:\n--------------------");
	for(int j=1; j<=100; j++)
	{
	if(i%j==0)
	System.out.println(j);
	}System.out.println("--------------------");
	}
	}
	}
