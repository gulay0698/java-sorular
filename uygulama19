package soru;
	public class soru19 {
		public static void bul(int[] data) {
			int sayi=data[0];
			for(int i=0; i<data.length; i++)
			{
				if(sayi<data[i])
					sayi=data[i];
			}
	System.out.println("en buyuk sayi: " + sayi);
		}
	public static void main(String[] args) {
		
	int [] dizi = {15,19,21,28,9};
			bul(dizi);
		}
	
	}
